<service>

    <!-- ID of the service. It should be unique across the Windows system-->
    <id>carsharing_parser_simple</id>
    <!-- Display name of the service -->
    <name>Parser SpringBoot Service (powered by WinSW)</name>
    <!-- Service description -->
    <description>S. saves information about cars once every 8 hours</description>

    <!-- Path to the executable, which should be started -->
    <executable>java</executable>
    <arguments>-jar %BASE%\carsharing-1.0.0.jar</arguments>

</service>
